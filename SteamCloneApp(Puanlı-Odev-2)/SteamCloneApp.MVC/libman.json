{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "AlertifyJS@1.13.1",
      "destination": "wwwroot/lib/AlertifyJS/",
      "files": [
        "alertify.min.js",
        "css/alertify.min.css"
      ]
    },

    {
      "provider": "cdnjs",
      "library": "bootstrap@5.3.0-alpha3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.min.css",
        "font/bootstrap-icons.min.css",
        "font/bootstrap-icons.min.css",
        "icons/facebook.svg",
        "icons/google.svg",
        "icons/linkedin.svg",
        "icons/twitter.svg",
        "icons/instagram.svg",
        "icons/github.svg",
        "icons/microsoft.svg",
        "icons/steam.svg",
        "icons/apple.svg",
        "icons/download.svg"

      ]
    }
  ]
}